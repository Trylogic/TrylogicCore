package tl.service{	import tl.ioc.IoCHelper;	public class Service implements IService	{		protected namespace lifecycle = "http://www.trylogic.ru/service/lifecycle";		protected var serviceInitialized : Boolean = false;		public function get initialized() : Boolean		{			return serviceInitialized;		}		public function Service()		{		}		public final function init() : void		{			IoCHelper.injectTo( this );			lifecycle::init();			serviceInitialized = true;		}		lifecycle function init() : void		{		}	}}